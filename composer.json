{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.3",
        "anahkiasen/former": "4.*",
        "laravel/framework": "5.6.*",
        "laravelcollective/html": "5.*",
        "vsmoraes/laravel-pdf": "1.0.1",
        "propaganistas/laravel-phone": "~2.0",
        "bugsnag/bugsnag-laravel": "1.*",
        "cerdic/css-tidy": "~v1.5",
        "thomaswelton/laravel-gravatar": "~1.0",
        "neitanod/forceutf8": "dev-master",
        "nicolaslopezj/searchable": "1.*",
        "chumper/datatable": "dev-develop",
        "chumper/zipper": "1.0.x",
        "cleverit/ubl_invoice": "1.*",
        "tymon/jwt-auth": "dev-develop",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "davibennun/laravel-push-notification": "dev-laravel5",
        "doctrine/dbal": "2.5.x",
        "ezyang/htmlpurifier": "~v4.7",
        "brozot/laravel-fcm": "^1.0",
        "barryvdh/laravel-debugbar": "3.1.*",
        "aws/aws-sdk-php": "~3.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "mremi/url-shortener": "^1.0",
        "intervention/image": "dev-master",
        "laravel/socialite": "3.0.11",
        "tedivm/fetch": "0.6.*",
        "laracasts/presenter": "dev-master",
        "jonnyw/php-phantomjs": "dev-fixes",
        "unisharp/laravel-filemanager": "^1.6",
        "unisharp/laravel-ckeditor": "^4.5",
        "torann/geoip": "^1.0",
        "maatwebsite/excel": "~2.0",
        "nwidart/laravel-modules": "^1.0",
        "pragmarx/google2fa-laravel": "^0.1.2",
        "predis/predis": "^1.1",
        "simshaun/recurr": "dev-master",
        "yajra/laravel-datatables-oracle": "~6.0",
        "turbo124/laravel-push-notification": "2.*",
        "websight/l5-google-cloud-storage": "dev-master",
        "flowjs/flow-php-server": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpspec/phpspec": "^4.0",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*",
        "fzaninotto/faker": "^1.5",
        "filp/whoops": "~2.0",
        "laravel/dusk": "~3.0",
        "laravel/tinker": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "repositories": [

        {
            "type": "vcs",
            "url": "https://github.com/hillelcoren/php-phantomjs"
        }
    ]
}
